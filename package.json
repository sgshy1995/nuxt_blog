{
  "name": "nuxt_blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:nuxt": "nuxt",
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts --watch server --exec babel-node --extensions .ts,.tsx & babel -w ./src --out-dir build --extensions .ts,.tsx",
    "product": "cross-env NODE_ENV=production nodemon server/index.ts --watch serve --exec babel-node --extensions .ts,.tsx ",
    "dev:both": "concurrently \"nuxt\" \"babel -w ./src --out-dir build --extensions .ts,.tsx\"",
    "dev:linux": "nuxt & babel -w ./src --out-dir build --extensions .ts,.tsx",
    "babel:build": "babel ./src --out-dir build --extensions .ts,.tsx",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "test-scsslint": "./node_modules/.bin/sass-lint 'client/styles/**/*.scss' -v -q",
    "typeorm:build": "babel ./src --out-dir build --extensions .ts,.tsx",
    "typeorm:build:watch": "babel -w ./src --out-dir build --extensions .ts,.tsx",
    "migration:create": "typeorm migration:create -n CreatePost",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "entity:create": "typeorm entity:create -n Post"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.15.5",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "ant-design-vue": "^1.7.6",
    "nuxt": "^2.15.8",
    "ua-parser-js": "^0.7.28",
    "vue-property-decorator": "^9.1.2",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@koa/cors": "^3.1.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript": "2.6.2",
    "@nuxt/typescript-build": "^2.1.0",
    "@types/koa-bodyparser": "^4.3.3",
    "@types/koa-generic-session": "^1.0.6",
    "@types/koa-router": "^7.4.4",
    "@types/koa-session": "^5.10.4",
    "@types/lodash": "^4.14.172",
    "@types/marked": "^3.0.1",
    "@types/node": "^16.7.10",
    "@types/ua-parser-js": "^0.7.36",
    "@types/vue-markdown": "^2.2.1",
    "@vue/babel-preset-app": "^4.5.13",
    "@vue/test-utils": "^1.2.1",
    "babel-jest": "^27.0.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "github-markdown-css": "^4.0.0",
    "jest": "^27.0.5",
    "koa": "^2.7.0",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-generic-session": "^2.1.1",
    "koa-json": "^2.0.2",
    "koa-router": "^10.1.1",
    "koa-session": "^6.2.0",
    "lodash": "^4.17.21",
    "marked": "^3.0.4",
    "moment": "^2.29.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "pg": "^8.7.1",
    "quill": "^1.3.7",
    "reflect-metadata": "^0.1.13",
    "sass-loader": "^10",
    "ts-jest": "^27.0.3",
    "typeorm": "^0.2.37",
    "vue-class-component": "^7.2.6",
    "vue-jest": "^3.0.4",
    "vue-markdown": "^2.2.4",
    "vue-quill-editor": "^3.0.6"
  }
}
