{
  "name": "nuxt_blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:both": "concurrently \"next dev\" \"babel -w ./src --out-dir build --extensions .ts,.tsx\"",
    "dev:linux": "next dev & babel -w ./src --out-dir build --extensions .ts,.tsx",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "test-scsslint": "./node_modules/.bin/sass-lint 'client/styles/**/*.scss' -v -q",
    "typeorm:build": "babel ./src --out-dir build --extensions .ts,.tsx",
    "typeorm:build:watch": "babel -w ./src --out-dir build --extensions .ts,.tsx",
    "migration:create": "typeorm migration:create -n CreatePost",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "ant-design-vue": "^1.7.6",
    "core-js": "^3.17.2",
    "nuxt": "^2.15.7",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@types/node": "^16.7.10",
    "@vue/babel-preset-app": "^4.5.13",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "concurrently": "^6.2.1",
    "jest": "^27.0.5",
    "node-sass": "^6.0.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sass-loader": "^10",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.2.1",
    "typeorm": "^0.2.37",
    "vue-class-component": "^7.2.6",
    "vue-jest": "^3.0.4"
  }
}
